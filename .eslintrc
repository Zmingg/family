{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "import": true
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-quotes": [
      2,
      "prefer-single"
    ]
  }
}